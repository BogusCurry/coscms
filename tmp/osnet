<VirtualHost *:80>
    ServerAdmin webmaster@example.com
    ServerName  osnet

    # Indexes + Directory Root.
    DirectoryIndex index.php
    DocumentRoot /home/dennis/www/osnet/htdocs
  
    <Directory /home/dennis/www/osnet/htdocs>
        RewriteEngine on
        RewriteBase /
        RewriteCond %{REQUEST_FILENAME} !-f
        RewriteCond %{REQUEST_FILENAME} !-d
        RewriteRule ^(.*)$ index.php?q=$1 [L,QSA]
    </Directory>
   
    ErrorLog  /home/dennis/www/osnet/logs/error.log
    CustomLog /home/dennis/www/osnet/logs/access.log combined
</VirtualHost>